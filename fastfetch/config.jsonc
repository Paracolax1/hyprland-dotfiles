{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "padding": {
      "right": 2
    }
  },
  "display": {
    "bar": {
      "char.elapsed": "/",
      "char.total": "/",
      "width": 20
    },
    "percent": {
      "type": 3,
      "color": {
        "green": "green",
        "yellow": "yellow"
      },
      "ndigits": 2
    },
    "key": {
      "width": 15
    },
    "color": "blink_",
    "separator": " ~> "
  },
  "modules": [
    "break",
    "break",
    "break",
    "break",
    {
      "type": "title",
      "format": "{6}{7}{#green}{8}{#}"
    },
    "separator",
    {
      "type": "os",
      "format": "{#blue}{2}{#}{?6}({#green}{6}{#}){?} {?12}{12}{?}"
    },
    "kernel",
    "shell",
    "terminal",
    {
      "type": "localip",
      "showMac": true,
      "compact": true
    },
    "uptime",
    "break",
    {
      "type": "cpu",
      "showPeCoreCount": true,
      "temp": true
    },
    "gpu",
    {
      "type": "memory",
      "key": "RAM"
    },
    {
      "type": "disk"
    },
    "break",
    {
      "type": "media",
      "format": "{5}: <{#underline_yellow}{1}{#}> {?3}by {#1}{3}{#}{?}"
    },
    {
      "type": "sound",
      "format": "{2:15} {5} {3}"
    },
    "break",
    "colors"
  ]
}
